{

  "description": "Better history view",
  "manifest_version": 2,
  "name": "TestRail Historian",
  "version": "1.0",
  "homepage_url": "https://github.com/cernyjan/TestRail-Historian",
  "icons": {
    "48": "icons/history-48.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/history-48.png",
      "32": "icons/history-48.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://testrail.quadient.group/*"],
      "js": ["jquery-3.2.1.min.js", "TestRail-Historian.js"]
    }
  ]

}